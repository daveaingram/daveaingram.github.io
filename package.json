{
  "name": "gatsby-starter-casper",
  "description": "GatsbyJS starter that themed with the Casper theme from Ghost.",
  "version": "1.0.3",
  "author": "Hays Clark <hays.clark@gmail.com>",
  "dependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-lodash": "^3.2.11",
    "classnames": "^2.2.5",
    "gatsby": "^1.9.224",
    "gatsby-cli": "^1.1.45",
    "gatsby-link": "^1.6.38",
    "gatsby-pagination": "^1.2.0",
    "gatsby-plugin-catch-links": "^1.0.19",
    "gatsby-plugin-feed": "^1.3.19",
    "gatsby-plugin-google-analytics": "^1.0.20",
    "gatsby-plugin-manifest": "^1.0.15",
    "gatsby-plugin-nprogress": "^1.0.13",
    "gatsby-plugin-offline": "^1.0.15",
    "gatsby-plugin-react-helmet": "^1.0.2",
    "gatsby-plugin-sharp": "^1.6.40",
    "gatsby-plugin-sitemap": "^1.2.14",
    "gatsby-plugin-twitter": "^1.0.18",
    "gatsby-remark-autolink-headers": "^1.4.13",
    "gatsby-remark-copy-linked-files": "^1.5.30",
    "gatsby-remark-images": "^1.5.56",
    "gatsby-remark-prismjs": "^1.2.17",
    "gatsby-remark-responsive-iframe": "^1.4.18",
    "gatsby-source-filesystem": "^1.5.26",
    "gatsby-transformer-json": "^1.0.16",
    "gatsby-transformer-remark": "^1.7.35",
    "lodash": "^4.17.5",
    "lodash-webpack-plugin": "^0.11.4",
    "moment": "^2.21.0",
    "npm": "^5.7.1",
    "react": "^15.6.1",
    "react-disqus-comments": "^1.0.3",
    "react-dom": "^15.6.1",
    "react-gravatar": "^2.6.3",
    "react-helmet": "^5.1.3",
    "react-md": "^1.2.11",
    "react-scroll": "^1.7.7",
    "react-share": "^2.0.0",
    "react-social-icons": "^2.8.1",
    "react-twitter-widgets": "^1.4.0"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "gh-pages": "^1.1.0",
    "prettier": "^1.11.1",
    "remark-cli": "^4.0.0",
    "remark-preset-lint-recommended": "^3.0.1",
    "standard-version": "^4.3.0",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^17.0.0",
    "write-good": "^0.11.3"
  },
  "keywords": [
    "gatsby",
    "blog",
    "casper",
    "starter"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:md",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public --ignore-pattern static .",
    "lint:css": "stylelint --fix 'src/**/*.css'",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "release": "standard-version -a",
    "deploy": "gatsby build && gh-pages -b master -d public"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
